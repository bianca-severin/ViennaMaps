<Window x:Class="ViennaMaps.Views.ChooseProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViennaMaps.Views"
        mc:Ignorable="d"
        Title="ChooseProfileWindow" Height="460" Width="620">
    <Grid  Background="#FF2B3545">
        <TextBlock HorizontalAlignment="Left" Margin="32,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF3ADEE8" TextAlignment="Justify" FontFamily="Nirmala UI" FontSize="20" FontWeight="Bold" LineStackingStrategy="BlockLineHeight"><Run Text="1. "/><Run Language="de-at" Text="Select"/><Run Text=" "/><Run Text="or create"/><Run Language="de-at" Text=" a new"/><Run Text=" "/><Run Text="profile:"/></TextBlock>
        <ComboBox x:Name="SelectProfileComboBox" HorizontalAlignment="Center" Margin="0,134,0,0" VerticalAlignment="Top" Width="460" FontFamily="Nirmala UI" FontSize="18" Foreground="#FF2B3545" Height="35" ItemsSource="{Binding ProjectName}"  SelectedItem="{Binding SelectedProject,UpdateSourceTrigger=PropertyChanged}">

        </ComboBox>
    </Grid>
</Window>
